pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                echo "Bulding"
            }
	}
        stage("Parallel stage"){
            Parallel{
                stage("running"){
                    steps{
                        echo "running"
                    }
                }
                stage("code scanning"){
                    steps{
                        echo "code scanning completed"
                    }
                }
            }
        }
        stage("evrythingbis done "){
            steps{
                echo "parallal stage has created and it's wornking"
                echo "this is the end of the pipeline"          
            }
        }
        }
    }
}
